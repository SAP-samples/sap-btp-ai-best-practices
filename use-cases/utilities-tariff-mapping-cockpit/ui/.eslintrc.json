{
  "extends": ["eslint:recommended", "plugin:react-hooks/recommended"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "react-refresh/only-export-components": "off"
  }
}
