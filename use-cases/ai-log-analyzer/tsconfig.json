{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "allowJs": true,
    "types": ["node", "@cap-js/cds-types"],
    "paths": {
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    }
  }
}
