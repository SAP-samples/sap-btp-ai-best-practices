{
  "name": "ai-rag-query-cap-fullstack-demo",
  "version": "1.0.0",
  "description": "AI RAG Query Demo for CAP (fullstack)",
  "type": "module",
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-ai-sdk/foundation-models": "^1.11.0",
    "@sap/cds": "^8",
    "@sap/xssec": "^4",
    "csv-parse": "^5.6.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0.3.0",
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/cds-compiler": "^5.9.2",
    "@sap/ux-specification": "^1.124.16",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds-tsx w --profile hybrid",
    "debug": "cds-tsx w --profile hybrid --debug",
    "build:db": "cds deploy --to hana:ai-rag-query-cap-fullstack-demo-db --auto-undeploy",
    "build": "mbt build",
    "deploy": "cf deploy mta_archives/ai-rag-query-cap-fullstack-demo_1.0.0.mtar"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "db": {
        "kind": "hana"
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "sapux": [
    "app/manage-science-data"
  ]
}