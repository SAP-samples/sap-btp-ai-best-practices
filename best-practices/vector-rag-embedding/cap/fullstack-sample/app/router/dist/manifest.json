{
  "_version": "1.49.0",
  "sap.app": {
    "id": "ui5.manage.sciencedata",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "type": "application",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [
        "",
        "en"
      ]
    },
    "dataSources": {
      "RagService": {
        "uri": "odata/v4/rag/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ScienceData-manage": {
          "signature": {
            "parameters": {
              "ScienceData.ID": {
                "renameTo": "ID"
              }
            },
            "additionalParameters": "allowed"
          },
          "semanticObject": "ScienceData",
          "action": "manage",
          "title": "{{appTitle}}",
          "info": "{{appInfo}}",
          "subTitle": "{{appSubTitle}}",
          "icon": "sap-icon://course-book",
          "indicatorDataSource": {
            "dataSource": "RagService",
            "path": "ScienceData/$count",
            "refresh": 1800
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": false,
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.115.1",
      "libs": {
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties",
        "settings": {
          "supportedLocales": [
            "",
            "en"
          ]
        }
      },
      "": {
        "dataSource": "RagService",
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "default": {
              "submit": "Auto"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ScienceDataList",
          "target": "ScienceDataList"
        },
        {
          "pattern": "ScienceData({key}):?query:",
          "name": "ScienceDataDetails",
          "target": "ScienceDataDetails"
        }
      ],
      "targets": {
        "ScienceDataList": {
          "type": "Component",
          "id": "ScienceDataList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "ScienceData",
              "initialLoad": true,
              "navigation": {
                "ScienceData": {
                  "detail": {
                    "route": "ScienceDataDetails"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "selectionMode": "Multi"
                  },
                  "actions": {
                    "Upload": {
                      "press": "ui5.manage.sciencedata.ext.CustomActions.onUpload",
                      "text": "Upload",
                      "enabled": true,
                      "visible": true
                    }
                  }
                }
              }
            }
          }
        },
        "ScienceDataDetails": {
          "type": "Component",
          "id": "ScienceDataDetails",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ScienceData"
            }
          }
        }
      },
      "config": {}
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}