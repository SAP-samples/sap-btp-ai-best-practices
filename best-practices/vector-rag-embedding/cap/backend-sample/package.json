{
  "name": "ai-rag-embedding-cap-backend-demo",
  "version": "1.0.0",
  "description": "AI RAG Embedding Demo for CAP (backend only)",
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/xssec": "^4",
    "csv-parse": "^5.6.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds-tsx w --profile hybrid",
    "debug": "cds-tsx w --profile hybrid --debug",
    "build:db": "cds deploy --to hana:ai-rag-embedding-cap-backend-demo-db --auto-undeploy"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "db": {
        "kind": "hana"
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
